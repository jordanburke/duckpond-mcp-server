{
  "name": "duckpond-mcp-server",
  "version": "0.3.4",
  "description": "MCP server for multi-tenant DuckDB management with R2/S3 storage",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "duckdb",
    "duckpond",
    "r2",
    "s3",
    "cloudflare",
    "multi-tenant",
    "analytics",
    "typescript"
  ],
  "author": "jordan.burke@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/jordanburke/duckpond-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/jordanburke/duckpond-mcp-server"
  },
  "type": "module",
  "bin": {
    "duckpond-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "validate": "pnpm format && pnpm lint && pnpm test && pnpm build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint ./src --fix",
    "lint:check": "eslint ./src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "build": "rimraf dist && cross-env NODE_ENV=production tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "serve:test": "tsx src/index.ts",
    "serve:test:http": "tsx src/index.ts --transport http",
    "prepublishOnly": "pnpm validate",
    "ts-types": "tsc"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.7",
    "@jordanburke/fastmcp": "3.26.700",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "commander": "^14.0.2",
    "debug": "^4.4.3",
    "duckpond": "^0.3.0",
    "express": "^5.2.1",
    "hono": "^4.11.3",
    "jsonwebtoken": "^9.0.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/node": "~24.10.4",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "^4.0.16",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "lib",
    "dist"
  ],
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
