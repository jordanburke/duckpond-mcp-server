{
  "name": "duckpond-mcp-server",
  "version": "0.2.0",
  "description": "MCP server for multi-tenant DuckDB management with R2/S3 storage",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "duckdb",
    "duckpond",
    "r2",
    "s3",
    "cloudflare",
    "multi-tenant",
    "analytics",
    "typescript"
  ],
  "author": "jordan.burke@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/jordanburke/duckpond-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/jordanburke/duckpond-mcp-server"
  },
  "type": "module",
  "bin": {
    "duckpond-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "validate": "pnpm format && pnpm lint && pnpm test && pnpm build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint ./src --fix",
    "lint:check": "eslint ./src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "build": "rimraf dist && cross-env NODE_ENV=production tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "serve:test": "tsx src/index.ts",
    "serve:test:http": "tsx src/index.ts --transport http",
    "prepublishOnly": "pnpm validate",
    "ts-types": "tsc"
  },
  "dependencies": {
    "@jordanburke/fastmcp": "3.21.0",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "commander": "^12.1.0",
    "debug": "^4.4.3",
    "duckpond": "^0.2.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@types/node": "^22.18.11",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "lib",
    "dist"
  ],
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d"
}
